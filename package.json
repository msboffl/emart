{
  "name": "@emart/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
    "generate:tree": "struct -i .next -i dist -i playwright-report -i test-results -i .nx -o project.tree",
    "admin:dev": "pnpm --filter admin dev",
    "admin:build": "pnpm --filter admin build",
    "admin:lint": "pnpm --filter admin lint",
    "admin:preview": "pnpm --filter admin preview",
    "admin:e2e": "pnpm --filter admin-e2e test",
    "admin:e2e:report": "pnpm --filter admin-e2e test:report",
    "web:dev": "pnpm --filter web dev",
    "web:build": "pnpm --filter web build",
    "web:lint": "pnpm --filter web lint",
    "web:e2e": "pnpm --filter web-e2e test",
    "web:e2e:report": "pnpm --filter web-e2e test:report",
    "gateway:dev": "pnpm --filter gateway dev",
    "gateway:build": "pnpm --filter gateway build",
    "auth:dev": "pnpm --filter auth dev",
    "auth:build": "pnpm --filter auth build"
  },
  "private": true,
  "dependencies": {
    "express": "^5.1.0",
    "next": "15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.22.0",
    "@nx/js": "20.8.1",
    "@playwright/test": "^1.52.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.31",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv": "^16.5.0",
    "eslint": "^9.22.0",
    "eslint-config-next": "15.3.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "nx": "20.8.1",
    "prettier": "^2.6.2",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.3.0",
    "tsup": "^8.5.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  },
  "packageManager": "pnpm@10.8.0"
}
